{
  "name": "@nuxt-themes/docus",
  "version": "1.15.0",
  "type": "module",
  "main": "./nuxt.config.ts",
  "packageManager": "pnpm@8.12.1",
  "repository": "nuxt-themes/docus",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "layouts",
    "plugins",
    "server",
    "nuxt.schema.ts",
    "app.vue",
    "app.config.ts",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .starters/default",
    "dev": "nuxi dev .starters/default",
    "build": "nuxi build .starters/default",
    "generate": "nuxi generate .starters/default",
    "preview": "nuxi preview .starters/default",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt-themes/elements": "^0.9.5",
    "@nuxt-themes/tokens": "^1.9.1",
    "@nuxt-themes/typography": "^0.11.0",
    "@nuxt/content": "^2.9.0",
    "@nuxthq/studio": "^1.0.5",
    "@vueuse/integrations": "^10.7.0",
    "@vueuse/nuxt": "^10.7.0",
    "focus-trap": "^7.5.4",
    "fuse.js": "^6.6.2"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.21.1",
    "@docsearch/css": "^3.5.2",
    "@docsearch/js": "^3.5.2",
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/algolia": "^1.10.1",
    "@types/node": "^20.10.4",
    "eslint": "^8.55.0",
    "nuxt": "^3.8.2",
    "release-it": "^16.3.0",
    "typescript": "^5.3.3",
    "vite-plugin-inspect": "^0.8.1",
    "vue": "^3.3.11"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vite": "*"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "axios",
        "vite"
      ]
    }
  },
  "license": "MIT",
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
